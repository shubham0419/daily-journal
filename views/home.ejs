<%- include("partials/header"); -%>
<div class="content">
  <h1 class="heading">Home</h1>
  <p><%= startingContent %></p>

  <% users.forEach(function(user){ %>
    <% user.blogs.forEach(function(blog){ %>

      <h1><%=blog.title%></h1>
      <p class="home-blogs">
        <%=blog.content.substring(0, 100) + " ..."%>
        <a href="/posts/<%=blog._id%>">Read More</a>
      </p>

    <% }) %>
  <% }) %>
</div>

<%- include("partials/footer"); -%>
